<form method="post" action="{{ path('ville_delete', {'id': ville.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ville.id) }}">
	<button class="btn btn-danger ml-5 mt-3">Supprimer la ville</button>

</form>
